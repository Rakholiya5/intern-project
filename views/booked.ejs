    <body></body>
       <section class="reserve" id="reserve">
            <div class="title">
                <h2><span>R</span>egister</h2>
                <p>Book your Table here with ease and comfort.</p>
            </div>
            <form action="/" method="POST">
               <div class="forms">
                    <div class="reserveForm">
                        <h3><b><i><%= name %></i></b>,</h3>
                        <h3>Your seats are booked Successfully.</h3>  
                        <h3>Please Remeber it</h3>
                        <h3>Table No: <b><%= table %></b></h3>
                        <h3>Members: <b><%= member %></b></h3>
                    </div>
                </div>
            </form>
       </section>
